{{#each operations~}}
  {{~#if (equalAny engine 2)}}
  {{else}}
    {{~#if hasInput~}}
      {{name}}Input,
    {{~/if~}}
  {{~#if (isAllTrue hasInternalInput includeInternal)~}}
    {{name}}InternalInput,
  {{~/if~}}
  {{~#if includeResponse~}}
    {{name}}Response,
  {{~/if~}}
  {{~#if includeResponseData~}}
    {{name}}ResponseData,
  {{~/if~}}
  {{/if}}
{{~/each}}